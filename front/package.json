{
	"name": "front",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@ckeditor/ckeditor5-build-classic": "^23.0.0",
		"@ckeditor/ckeditor5-build-decoupled-document": "^23.0.0",
		"@ckeditor/ckeditor5-react": "^2.1.0",
		"@reduxjs/toolkit": "^1.3.6",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"antd": "^4.2.2",
		"antd-mask-input": "^0.1.13",
		"axios": "^0.19.2",
		"babel-plugin-import": "^1.13.0",
		"ckeditor5-custom-build": "file:ckeditor",
		"moment": "^2.25.3",
		"node-sass": "^4.14.1",
		"prop-types": "^15.7.2",
		"react": "^16.13.1",
		"react-app-rewired": "^2.1.6",
		"react-copy-to-clipboard": "^5.0.3",
		"react-dom": "^16.13.1",
		"react-hook-form": "^5.7.2",
		"react-redux": "^7.2.0",
		"react-responsive": "^8.1.0",
		"react-router-dom": "^5.2.0",
		"react-scripts": "3.4.1",
		"redux": "^4.0.5",
		"redux-persist": "^6.0.0",
		"redux-saga": "^1.1.3",
		"styled-components": "^5.1.0"
	},
	"scripts": {
		"start": "react-app-rewired start",
		"build": "react-app-rewired build",
		"test": "react-app-rewired test",
		"test:ci": "react-app-rewired test --colors --coverage",
		"eject": "react-scripts eject"
	},
	"prettier": {
		"tabWidth": 2,
		"useTabs": true,
		"semi": false,
		"singleQuote": true,
		"trailingComma": "es5",
		"jsxBracketSameLine": true
	},
	"jest": {
		"setupFilesAfterEnv": [
			"@testing-library/jest-dom/extend-expect"
		],
		"moduleNameMapper": {
			"^~/(.*)": "<rootDir>/src/$1"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"eslint . --fix"
		]
	},
	"devDependencies": {
		"@ckeditor/ckeditor5-comments": "^23.1.0",
		"@ckeditor/ckeditor5-dev-utils": "^23.6.1",
		"@ckeditor/ckeditor5-dev-webpack-plugin": "^23.6.1",
		"babel-plugin-root-import": "^6.5.0",
		"customize-cra": "^0.9.1",
		"eslint": "6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.3",
		"husky": "^4.3.0",
		"lint-staged": "^10.3.0",
		"prettier": "^2.0.5",
		"uglifyjs-webpack-plugin": "^2.2.0"
	}
}
